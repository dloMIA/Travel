
<h1>All users</h1>

<br></br>

<div class="main">
<%= form_tag users_path, :method => 'get' do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search" %>
  </p>
<% end %>


<table>
<% @users.each do |user| %>
    <tr>
      	<td><%= user.name %></td>
		<td><%= user.email %></td> 
	</tr>
  <% end %>
</table>
